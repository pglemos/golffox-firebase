# Configuração do Firebase (Cliente)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Configuração do Firebase Cloud Messaging (FCM)
NEXT_PUBLIC_FIREBASE_VAPID_KEY=your_vapid_key_here

# Configuração dos Emuladores Firebase (desenvolvimento)
NEXT_PUBLIC_USE_FIREBASE_EMULATORS=false
FIREBASE_EMULATOR_HOST=localhost
FIREBASE_AUTH_EMULATOR_HOST=localhost:9099
FIRESTORE_EMULATOR_HOST=localhost:8080
FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199

# Configuração do Firebase Admin (servidor)
FIREBASE_ADMIN_PROJECT_ID=your_project_id
FIREBASE_ADMIN_CLIENT_EMAIL=your_service_account_email
FIREBASE_ADMIN_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----"

# Configuração do Google Maps (opcional)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Configuração do Gemini AI (opcional)
GEMINI_API_KEY=your_gemini_api_key_here

# Configuração do ambiente
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000